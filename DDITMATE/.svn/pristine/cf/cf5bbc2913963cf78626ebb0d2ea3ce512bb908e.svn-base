package blame.vo;

public class BlameVO {
	private int blaNo;
	private String blaContents;
	private String blaDate;
	private String memId;
	private int mentNo;
	private int boNo;
	private String cateCode;
	private String blaCate;
	
	public int getBlaNo() {
		return blaNo;
	}
	public void setBlaNo(int blaNo) {
		this.blaNo = blaNo;
	}
	public String getBlaContents() {
		return blaContents;
	}
	public void setBlaContents(String blaContents) {
		this.blaContents = blaContents;
	}
	public String getBlaDate() {
		return blaDate;
	}
	public void setBlaDate(String blaDate) {
		this.blaDate = blaDate;
	}
	public String getMemId() {
		return memId;
	}
	public void setMemId(String memId) {
		this.memId = memId;
	}
	public int getMentNo() {
		return mentNo;
	}
	public void setMentNo(int mentNo) {
		this.mentNo = mentNo;
	}
	public int getBoNo() {
		return boNo;
	}
	public void setBoNo(int boNo) {
		this.boNo = boNo;
	}
	public String getCateCode() {
		return cateCode;
	}
	public void setCateCode(String cateCode) {
		this.cateCode = cateCode;
	}
	public String getBlaCate() {
		return blaCate;
	}
	public void setBlaCate(String blaCate) {
		this.blaCate = blaCate;
	}
	@Override
	public String toString() {
		return "BlameVO [blaNo=" + blaNo + ", blaContents=" + blaContents + ", blaDate=" + blaDate + ", memId=" + memId
				+ ", mentNo=" + mentNo + ", boNo=" + boNo + ", cateCode=" + cateCode + ", blaCate=" + blaCate + "]";
	}
	

	}
	